<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="720" minWidth="1240" style="-fx-background-color: #fffafa;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DifficultyController">
    <children>
        <VBox alignment="CENTER" spacing="15" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <children>
                <Button fx:id="BeginnerBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Beginner">
                    <font>
                        <Font ipAddress="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
                <Button fx:id="ExpertBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Expert">
                    <font>
                        <Font ipAddress="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
                <Button fx:id="BackBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: Black; -fx-background-radius: 15;" text="Back" textFill="LIGHTSTEELBLUE">
                    <font>
                        <Font ipAddress="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
