            <Label fx:id="ScoreLbl" text="SCORE: ">
                <font>
                    <Font name="Verdana Bold" size="50.0"/>
                </font>
            </Label>
        </HBox>
        <HBox alignment="TOP_RIGHT" layoutX="14.0" layoutY="12.0" prefHeight="85.0" prefWidth="500.0"
              AnchorPane.topAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <Button fx:id="Quit" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#Quitgame"
                    style="-fx-background-color: #b0c4de; -fx-background-radius: 5;" text="Quit">
