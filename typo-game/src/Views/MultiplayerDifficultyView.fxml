<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>
<AnchorPane fx:id="anchor" minHeight="720" minWidth="1240" style="-fx-background-color: #fffafa;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MultiplayerDifficultyController">
    <children>
        <VBox alignment="CENTER" spacing="15" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <children>
                <Label fx:id="lblName" text="Lobby Name:" textFill="WHITE">
                    <font>
                        <Font size="16.0" />
                    </font></Label>
                <Label fx:id="countdownLbl" text="" textFill="WHITE">
                    <font>
                        <Font size="16.0" />
                    </font></Label>
                <TextField fx:id="textField" prefHeight="39.0" maxWidth="540.0" />
                <Button fx:id="EasyBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Easy">
                    <font>
                        <Font name="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
                <Button fx:id="MediumBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Medium">
                    <font>
                        <Font name="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
                <Button fx:id="HardBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Hard">
                    <font>
                        <Font name="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
                <Button fx:id="BackBtn" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: Black; -fx-background-radius: 15;" text="Back" textFill="LIGHTSTEELBLUE">
                    <font>
                        <Font name="Tahoma Bold" size="16.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
