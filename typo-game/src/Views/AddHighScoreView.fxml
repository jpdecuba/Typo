<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="720.0" prefWidth="1240.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddHighScoreController">
    <children>
        <VBox alignment="CENTER" spacing="15" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="300" AnchorPane.rightAnchor="300" AnchorPane.topAnchor="0">
            <children>
                <Label id="Score" alignment="CENTER" prefHeight="27.0" prefWidth="400.0" text="Game Over!" textFill="WHITE">
                    <font>
                        <Font name="Verdana Bold" size="30.0" />
                    </font>
                </Label>
                <Label id="Score" fx:id="scoreLbl" alignment="CENTER" prefHeight="27.0" prefWidth="400.0" text="Score:" textFill="WHITE">
                    <font>
                        <Font name="Verdana" size="30.0" />
                    </font>
                </Label>
                <TextField fx:id="textField" prefHeight="39.0" prefWidth="114.0" />
                <HBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="15">
                    <children>
                        <Button id="Submit" fx:id="btnHSsubmit" mnemonicParsing="false" onAction="#btnSubmit" prefHeight="60.0" prefWidth="262.5" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Submit" textAlignment="CENTER">
                            <font>
                                <Font name="Tahoma Bold" size="16.0" />
                            </font>
                        </Button>
                        <Button id="Cancel" fx:id="btnHSCancel" mnemonicParsing="false" onAction="#btnCancel" prefHeight="60.0" prefWidth="262.5" style="-fx-background-color: b0c4de; -fx-background-radius: 15;" text="Cancel" textAlignment="CENTER">
                            <font>
                                <Font name="Tahoma Bold" size="16.0" />
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
