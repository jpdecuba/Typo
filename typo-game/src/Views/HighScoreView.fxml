<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1240.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.HighScoreController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="15" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15">
               <children>
                  <Button fx:id="btnMode" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#btnModeClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de" text="Singleplayer" />
               </children></HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15">
               <children>
                  <Button fx:id="btnBeginner_Easy" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de" text="Beginner" />
                  <Button fx:id="btnExpert_Normal" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de" text="Expert" />
                  <Button fx:id="btnHard" alignment="CENTER" mnemonicParsing="false" onAction="#btnClick" prefHeight="60.0" prefWidth="540.0" style="-fx-background-color: b0c4de" text="Hard" />
               </children></HBox>
            <ListView fx:id="lvHighscores" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
